{
  "compilerOptions": {
    "lib": ["ES2015", "DOM"],
    "moduleResolution": "Node",
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "for_debugging",
    "declaration": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@lib-apis": ["./apis/"],
      "@lib-apis/*": ["./apis/*"]
    }
  },
  "tsc-alias": {
    "verbose": true
  }
}
