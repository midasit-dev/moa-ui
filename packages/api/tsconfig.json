{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "for_debugging",
    "declaration": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@lib-apis": ["./apis/"],
      "@lib-apis/*": ["./apis/*"],
      "@lib-types": ["./types/"],
      "@lib-types/*": ["./types/*"],
      "@lib-utils": ["./utils/"],
      "@lib-utils/*": ["./utils/*"]
    }
  },
  "tsc-alias": {
    "verbose": true
  }
}
